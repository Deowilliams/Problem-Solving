#include<iostream>
using namespace std;
int main()
{
    int x;
    cin>>x;
    int arr[x];
    for(int i=0;i<x;i++)
    {
        cin>>arr[i];
    }
    int k=x/2;
    while(k!=0){
    int y=arr[0];
    for(int i=0;i<x-1;i++)
    {
        arr[i]=arr[i+1];
    }
    arr[x-1]=y;
    k--;
    }
    for(int i=0;i<x;i++)
    {
        cout<<arr[i]<<" ";
    }
}

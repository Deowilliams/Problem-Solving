class Solution {
public:
    int maxArea(vector<int>& h) {
        int n=h.size();
        int l=0;
        int r=n-1;
        int a=0;
        while(l<r)
        {
            a=max(a,min(h[l],h[r])*(r-l));
            if(h[l]<h[r])
            l++;
            else
            r--;  
        }
        return a;
    }
};
